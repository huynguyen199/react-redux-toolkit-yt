  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
